(defun robot-accelerometer (&optional (cnt 1)
                                      (acc (instantiate float-vector 3)))
  (dotimes (i cnt)
    (v+ acc
        (cout-float-vector
         (wb_accelerometer_get_values (get 'accelerometer :device)) 3)
        acc)
    )
  (scale (/ 1.0 cnt) acc))
(defun robot-gyro nil (cout-float-vector (wb_gyro_get_values (get 'gyro :device)) 2))

